{
  "name": "timer",
  "version": "1.0.0",
  "private": true,
  "license": "GNU GPLv2",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --root=src",
    "dev": "ts-node-esm src/main.ts",
