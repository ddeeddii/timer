    "watch": "nodemon --ignore ./db/ --exec ts-node-esm src/main.ts",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "cron": "^2.1.0",
    "discord.js": "^14.3.0",
    "discordx": "^11.1.12",
    "luxon": "^3.0.3",
    "node-json-db": "^2.1.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/cron": "^2.0.0",
    "@types/luxon": "^3.0.1",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "4.8.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
